var async = require('async'),
    redis = require('redis').createClient(),
    await = require('asyncawait/await'),
    asyncFunc = require('asyncawait/async');

module.exports = asyncFunc (function(options) {
    options.cards.map(function(card) {
        await (redis.sadd.bind(redis, 'table:' + options.table.id + ':deck', card));
    });
});
